:@template":(// rgba(0,0,0,.7-.8)
.qtip-default,
#body-content .nivoSlider .nivo-caption {
    background: darken(#eaeaea, 70%);
    //background: url(data:image/png;base64,);
}

.qtip-default {
	border-color: black !important;
	background: #333 !important;
	padding: 3px 5px;
	.qtip-content {
		color: #eee !important;
		font-size: 13px;
	}
}

// rgba(0,0,0,.06)
.bfi_accordion li > h4,
.bfi_tabs .tab-title h4,
.bfi_tabs .tab-body li,
.bfi_tabs.vertical .tab-body,
.bfi_highlight,
.dropcaps,
.bfi_toggle h4,
.bfi_infobox,
#respond,
.must_be_logged_in,
.titlebox,
body.error404 #body-content > .container > .columns,
.author-box > div,
#infscr-loading, .share-buttons,
#comments li,
.filters a.button.filter.not-selected,
body.single-portfolio_item .related-post > div,
.widget.widget_calendar #wp-calendar tbody,
.bfi_toggle > div,
hr
{
    background: darken(#eaeaea, 6%);
    //background: darken(#ededed, %8);
    //background: url(data:image/png;base64,);
}

// rgba(0,0,0,.4-.5)
.fancybox-skin .fancybox-title span,
.widget.widget_calendar #wp-calendar th {
    background: darken(#eaeaea, 50%);
    //background: url(data:image/png;base64,);
}

// rgba(0,0,0,.1)
#top-menu > ul > li > a:hover, 
#top-menu > ul > li > a.selected,
#main-menu > ul > li > a:hover,
#main-menu > ul > li > a.selected {
    background: url(data:image/png;base64,);
}

// rgba(255,255,255,.8)
.sharrre .box,
iframe.fb_ltr {
    background: lighten(darken(#eaeaea, 6%), 80%);//#ccc;
    //background: url(data:image/png;base64,);
}

// rgba(0,0,0,0.02)
#comments li.odd {
    background: darken(#eaeaea, 2%);//#fafafa;
    //background: url(data:image/png;base64,);
}



.widget li {
    border-color: darken(#eaeaea, 20%);
    ul {
        border-color: darken(#eaeaea, 20%);
    }
}

.sharrre {
    .box {
        border-color: #fafafa;
        .count:after {
            border-top-color: #ccc;
        }
        .share {
            text-shadow: 0px 1px 0px #a6a6a6;
        }
        i {
            text-shadow: 0px 1px 0px #a6a6a6;
        }
    }
}

#comments {
    li {
        &.bypostauthor {
            border-color: #fafafa;
        }
    }
}

body.page-template-template-sitemap-php {
    .sitemap {
        li {
            border-bottom: 1px solid #f5f5f5;
        }
    }
}

#heading-box-container {
	.one-third.column {
		.button {
			text-shadow: 0 1px #bfbfbf;
		}
	}
}


a.fancybox, a.lightbox {
    &:before {
        content: '' !important;
        display: none !important;
    }
}

.sidebar {
    form.search {
        input {
            width: 100% !important;
        }
    }
    form.signup {
        input {
            width: 100% !important;
        }
    }
}

.has-cta a.icon-large:before {
    content: '' !important;
    display: none !important;
}

a {
    text-decoration: none !important;
}

blockquote:before {
    color: #ccc !important;
    left: 10px !important;
    filter: none !important;
