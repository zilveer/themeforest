    li:before {
        @include font-awesome();
        content: "\f105";
        margin-right: 5px;
    }
    > hr {
        margin-bottom: 15px;
    }
    
    // language switcher
    &.widget_bfiwidgetlanguageswitchermodel {
        .language_switcher {
            text-align: center;
        }
    }
    // portfolio widget
    &.widget_bfiwidgetportfoliomodel {
        li:before {
            content: '';
        }
        .lightbox, .fancybox {
            width: 100%;
            height: auto;
            font-size: 100%;
            margin-bottom: 0;
            &:before {
                font-size: 3em;
            }
            img {
                width: 100%;
            }
        }
        div {
            margin-top: 5px;
        }
    }
    // calendar widget
    &.widget_calendar {
        #wp-calendar {
            width: 100%;
            text-align: center;
            margin-bottom: 20px;
            caption {
                font-weight: bold;
                margin-bottom: 5px;
                &:before {
                    @include font-awesome();
                    content: "\f073";
                    margin-right: 5px;
                }
            }
            td, th {
                line-height: 30px;
            }
            th {
                background: rgba(0,0,0,.5); //#999;
                color: white;
            }
            tr {
                border-bottom: 1px solid #eee;
            }
            tbody {
                tr:nth-child(even) {
                    background: white;
                }
                background: rgba(255,255,255,.8);//#fff;
                tr:last-child {
                    border: 0;
                }
            }
            tfoot {
                a {
                    margin-top: 5px;
                }
                tr {
                    border: 0;
                }
            }
            td a {
                display: block;
                text-decoration: none;
                font-weight: bold;
                line-height: 31px;
                &:hover {
                    background: #666;
                    color: white;
                }
            }
        }
    }
    // twitter
    &.widget_bfiwidgettwittermodel {
        .bfi-twitter-widget {
            padding-left: 30px;
            li {
                position: relative;
                &:before {
                    content: "\f099";
                    font-size: 150%;
                    left: -30px;
                    position: absolute;
                }
            }
        }
        .twitter-follow-button {
            margin-bottom: 10px;
        }
    }
    // recent comments
    &.widget_recent_comments {
        li:before {
            content: "\f075";
        }
    }
    // RSS feed
    &.widget_rss {
        > h4 {
            > .cufon {
                display: none !important;
            }
