}

// font awesome adjustment for lists. the spacing isn't correct
ul.icons {
	margin-left: 30px;
}

blockquote {
	padding-left: 60px;
	&:before {
		position: absolute;
		font-family: FontAwesome;
		left: 22px;
		content: "\f10d";
		@include opacity(.3);
		font-style: normal;
		font-size: 40px;
		margin-top: 10px;
		@media only screen and (max-width: 767px) {
			left: 12px;
		}
	}
}

.bfi_accordion {
	li {
		position: relative;
		overflow: hidden;
		> h4 {
			background: rgba(0, 0, 0, .06);
			padding: 8px 20px;
			cursor: pointer;
			&:after {
				content: '\f067';
				font-family: FontAwesome;
				font-size: 14px;
				float: right;
			}
		}
		> h4.selected {
			&:after {
				content: '\f068';
				font-family: FontAwesome;
				font-size: 14px;
				float: right;
			}
		}
		> div {
			padding: 15px 20px;
			display: none;
			> p:last-child {
			    margin-bottom: 0;
			}
		}
	}
}

.bfi_tabs {
	.tab-title {
		display: block;
		margin: 0;
		li {
			float: left;
			display: block;
			margin: 0;
		}
		h4 {
			float: left;
			background: rgba(0, 0, 0, .06);
			padding: 8px 20px;
			cursor: pointer;
			margin-right: 5px;
			margin-bottom: 0;
		}
	}
	.tab-body {
		clear: both;
		border-top-width: 4px;
		border-top-style: solid;
		li {
			padding: 15px 25px;
			background: rgba(0, 0, 0, .06);
			> div > p:last-child {
			    margin-bottom: 0;
			}
		}
	}
}
.bfi_tabs.vertical {
	.tab-title {
		float: left;
		width: 150px;
		h4 {
			width: 130px;
			padding: 8px 10px;
			margin-right: 0;
			margin-bottom: 5px;
		}
	}
	.tab-body {
		background: rgba(0,0,0,.06);
		clear: none;
		overflow: hidden;
		border-left-width: 4px;
		border-left-style: solid;
		border-top-width: 0;
		li {
			background: none;
		}
	}
}

// override ddsmoothmenu's submenu width
.ddsmoothmenu ul li ul li a {
