        .rssSummary {
            margin-top: 10px;
        }
        cite {
            margin-top: 5px;
            display: block;
            font-size: 90%;
            font-style: italic;
        }
        li:before {
            content: "\f09e";
        }
    }
    // archive
    &.widget_archive {
        li:before {
           content: "\f073"; 
        }
    }
    // fix for recent entries
    &.widget_recent_entries {
        li:before {
            margin-right: 0;
        }
    }
    // categories
    &.widget_categories {
        li:before {
            content: "\f07c";
        }
    }
    // recent blog posts
    &.widget_recent_entries {
        li:before {
            content: "\f040";
        }
    }
    // pages list
    &.widget_pages {
        li:before {
            content: "\f0f6";
        }
    }
    &.widget_bfiwidgetflickrmodel {
