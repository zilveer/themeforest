}

// this class is the stretched background. We add shadows to this since we
// can't on the body
.backstretch {
	&:after {
		content: '';
		position: fixed;
		top: 0;
		bottom: 0;
		left: 0;
		right: 0;
		z-index: 1;
	}
}

* {
	color: $font-color;
}

img {
	display: block;
}


#second-menu {
	text-align: right;
	position: absolute;
	bottom: 0;
	right: 0;
	margin-right: 0;
	@media only screen and (max-width: 767px) {
		position: relative;
		text-align: center;
		bottom: auto;
		right: auto;
	}
}


#heading-box {
	position: relative;
	&:before {
		display:block;
	    content:'';
	    position:absolute;
		z-index: -1;
		top: -1px;
		left: -1px;
		right: -1px;
		bottom: -1px;
		@include embossed();
		@media only screen and (max-width: 767px) {
			@include box-shadow(none);
		}
	}
}

#heading-menu-container {
	@media only screen and (max-width: 767px) {
		padding-top: 30px;
	}
}

#logo {
	margin-bottom: 20px;
}



