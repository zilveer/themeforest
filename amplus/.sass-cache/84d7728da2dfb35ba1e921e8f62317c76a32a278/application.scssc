	background: rgba(0,0,0,.06);
}


// togglers
.bfi_toggle {
	margin-bottom: 20px;
	h4 {
		background: rgba(0, 0, 0, .06);
		padding: 8px 20px;
		cursor: pointer;
		margin-bottom: 0;
		&:after {
			content: '\f067';
			font-family: FontAwesome;
			font-size: 14px;
			float: right;
		}
	}
	> div {
		background: rgba(0, 0, 0, .03);
		padding: 15px 25px;
	}
	&.open {
		h4 {
			&:after {
				content: '\f068';
				font-family: FontAwesome;
				font-size: 14px;
				float: right;
			}
		}
	}
}


