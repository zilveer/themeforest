<wpml-config>
  <custom-types>
    <custom-type translate="1">jaw-portfolio</custom-type>
    <custom-type translate="1">jaw-faq</custom-type>
    <custom-type translate="1">jaw-team</custom-type>
    <custom-type translate="1">jaw-testimonial</custom-type>
  </custom-types>
  <taxonomies>
    <taxonomy translate="1">jaw-portfolio-category</taxonomy>
    <taxonomy translate="1">jaw-faq-category</taxonomy>
    <taxonomy translate="1">jaw-team-category</taxonomy>
    <taxonomy translate="1">jaw-testimonial-category</taxonomy>
  </taxonomies>
  <custom-fields>
    <!-- Portfolio -->
    <custom-field action="copy">portfolio_type</custom-field>
    <custom-field action="copy">_portfolio_image</custom-field>
    <custom-field action="copy">_portfolio_gallery</custom-field>
    <custom-field action="copy">_portfolio_video_link</custom-field>
    <custom-field action="copy">_portfolio_link</custom-field>
    <custom-field action="copy">_portfolio_link_target</custom-field>
    <custom-field action="copy">_portfolio_sound</custom-field>
    <!-- Testimonial -->
    <custom-field action="translate">_testimontial_possition</custom-field>
    <custom-field action="copy">_testimontial_photo</custom-field>
    <!-- Team -->
    <custom-field action="translate">_team_possition</custom-field>
    <custom-field action="copy">_team_photo</custom-field>
    <custom-field action="copy">_team_mail</custom-field>
    <custom-field action="copy">_team_fb</custom-field>
    <custom-field action="copy">_team_tw</custom-field>
    <custom-field action="copy">_team_gp</custom-field>
    <custom-field action="copy">_team_lin</custom-field>
    <custom-field action="copy">_team_bl</custom-field>
    <!-- Posts -->
    <custom-field action="copy">_layout</custom-field>
    <custom-field action="copy">_sidebar_left1</custom-field>
    <custom-field action="copy">_sidebar_right1</custom-field>
    <custom-field action="copy">_use_featured</custom-field>
    <custom-field action="copy">_post_gallery</custom-field>
    <custom-field action="copy">_post_video_link</custom-field>
    <custom-field action="copy">post_connect_woo</custom-field>
    <!-- Post - rating -->
    <custom-field action="translate">fw_rating_title</custom-field>
    <custom-field action="translate">fw_rating_desc</custom-field>
    <custom-field action="copy">fw_rating_position</custom-field>
    <custom-field action="copy">fw_rating</custom-field>
    <custom-field action="copy">fw_rating_show_desc</custom-field>
    <custom-field action="copy">fw_rating_overal</custom-field>
    <custom-field action="copy">fw_rating_user_edit</custom-field>
    <custom-field action="copy">fw_rating_user_count</custom-field>
    <!-- Page -->
    <custom-field action="copy">_display_page_name</custom-field>
    <custom-field action="copy">_page_breadcrumbs</custom-field>
    <custom-field action="copy">show_comments_page</custom-field>
  </custom-fields>
  <admin-texts>
    <key name="goodstore_options">
      <key name="wrong_login_pageid"/>
      <key name="top_bar_span_text"/>
      <key name="footer_text"/>
      <key name="error_custom_html"/>
    </key>
  </admin-texts>
</wpml-config>