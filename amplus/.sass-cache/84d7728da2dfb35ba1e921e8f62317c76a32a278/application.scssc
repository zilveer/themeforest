// menu
#top-menu {
	visibility: visible;
	a, a:visited {
		border: none;
		text-align: left;
		font-size: 18px;
	}
	> ul > li > a:hover, > ul > li > a.selected {
		background: rgba(0, 0, 0, .1);
	}
	$vpadding: 7px;
	$hpadding: 18px;
	$hborder: 4px;
	> ul > li > a {
		line-height: 32px;
		padding: $vpadding $hpadding;
		@media only screen and (max-width: 960px) and (min-width: 767px) {
			padding-left: 10px;
			padding-right: 10px;
		}
	}
	> ul > li.current-menu-item > a {
		border-top: $hborder solid red;
		padding-top: $vpadding - $hborder;
	}
	> ul.has-cta > li:last-child a {
		padding: $vpadding $hpadding;
		padding-bottom: $vpadding - 3;
		border: none;
		float: right;
		width: 200px - 30px;
		text-align: left;
		&:before {
			float: right;
			padding-top: 3px;
		}
		&:hover {
			@include opacity(.85);
		}
	}
}
#main-menu {
	a {
		border: none;
		text-align: left;
	}
	$vpadding: 7px;
	$hpadding: 18px;
	$hborder: 4px;
	> ul > li > a {
		line-height: 32px;
		font-size: 16px;
		padding: $vpadding $hpadding;
		@media only screen and (max-width: 960px) and (min-width: 767px) {
			padding-left: 10px;
			padding-right: 10px;
		}
	}
	> ul > li.current-menu-item > a {
		border-bottom: $hborder solid red;
		padding-bottom: $vpadding - $hborder;
	}
	a.downarrow:after, a.rightarrow:after {
		// content: ' Â»';
		content: '\f101';
		font-family: fontAwesome;
		margin-left: 10px;
		font-size: 110%;
	}
	> ul > li > ul {
		margin-top: -$hborder;
		border-top: $hborder solid red;
		a, a:visited {
			padding: 10px 15px;
			color: #444;
			background: #eee;
			border-bottom: 1px solid #ddd;
		}
		a:hover, a.selected {
			background: #e0e0e0;
			color: red;
		}
	}
	> ul {
		@media only screen and (max-width: 767px) {
			display: none;
		}
	}
	> ul > li > a:hover, > ul > li > a.selected {
		background: rgba(0, 0, 0, .1);
	}
}

#body-content {
