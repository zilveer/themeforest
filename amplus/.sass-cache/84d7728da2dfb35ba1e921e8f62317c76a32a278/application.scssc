@import "compass";
@import "compass/css3/transition";
@import "compass/css3/box-shadow";
@import "compass/css3/opacity";
@import "compass/css3/box-sizing";

// MIXINS
@mixin embossed($boxshadow:0) {
	@if $boxshadow == 0 {
		@include box-shadow(inset 1px 1px 0px rgba(#fff, .3), 
						inset -1px -1px 0px rgba(#000, .2));
	} @else {
		@include box-shadow(inset 1px 1px 0px rgba(#fff, .3), 
						inset -1px -1px 0px rgba(#000, .2), $boxshadow);
	}
}
@mixin embossed-notop($boxshadow:0) {
	@if $boxshadow == 0 {
		@include box-shadow(inset 1px 0px 0px rgba(#fff, .3), 
						inset -1px -1px 0px rgba(#000, .2));
	} @else {
		@include box-shadow(inset 1px 0px 0px rgba(#fff, .3), 
						inset -1px -1px 0px rgba(#000, .2), $boxshadow);
	}
}

@mixin font-awesome() {
    font-family: FontAwesome;
    font-weight: normal;
    font-style: normal;
    text-decoration: inherit;
    display: inline;
    width: auto;
    height: auto;
    line-height: normal;
    vertical-align: baseline;
    background-image: none!important;
    background-position: 0 0;
    background-repeat: repeat;
    @include opacity(.7);
}

a {
    text-decoration: none;
}

a,
body #body-content,
#top-menu a, 
#top-menu a:visited,
#main-menu a,
#body-content .nivo-controlNav,
#heading-box .nivo-controlNav a,
.bfi_accordion li > h4,
.bfi_tabs .tab-title h4,
i, // animate font awesome transitions
.bfi_toggle h4,
