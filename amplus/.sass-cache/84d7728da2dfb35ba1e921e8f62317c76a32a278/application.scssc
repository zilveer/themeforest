#comments {
    li {
        background: rgba(0,0,0,.06);
        padding: 10px 10px 0;
        &.odd {
            background: rgba(0,0,0,.02);
        }
        &.bypostauthor {
            border: 1px solid rgba(0,0,0,.2);
        }
        img.avatar {
            width: auto;
            height: auto;
            float: left;
            margin-right: 20px;
        }
        > div {
            overflow: hidden;
        }
        .comment-reply-edit {
            float: right;
            .comment-reply-link {
                font-size: 80%;
            }
            .comment-edit {
                font-size: 80%;
                margin-left: 15px;
            }
        }
        .comment-name {
            margin-bottom: 10px;
            display: block;
            font-weight: bold;
            small {
                font-weight: normal;
            }
        }
        hr {
            margin-bottom: 10px;
        }
        
        // depth reply margins
        margin-left: 0 0 10px 120px;
        &.depth-1 {
            margin-left: 0;
        }
        &.depth-2 {
            margin-left: 30px;
        }
        &.depth-3 {
            margin-left: 60px;
        }
        &.depth-4 {
            margin-left: 80px;
        }
        &.depth-5 {
            margin-left: 100px;
        }
        &.depth-6 {
            margin-left: 110px;
        }
        &.depth-7 {
            margin-left: 120px;
        }
    }
    > ul {
        margin-bottom: 10px;
        > ul.children {
            margin-bottom: 20px;
        }
    }
    ul.children {
        margin: 0;
        font-size: 100%;
    }
}



// facebook box
iframe.fb_ltr {
    background: rgba(255,255,255,.8);
}


.filters {
    span {
        margin-right: 20px;
        @media only screen and (max-width: 767px) {
            display: none;
        }
    }
    a.button.filter {
        margin-right: 10px;
        padding: 3px 10px;
        background: rgba(0,0,0,.06);
        color: #555;
        font-weight: normal;
        &:hover {
            background: #555;
            color: white;
        }
        &.selected {
            cursor: default;
        }
    }
}


article.portfolio {
    @include transition-timing-function(ease-in);
    @include opacity(1);
	p {
	    font-size: 90%;
	    margin-bottom: 0;
	}
	.fancybox, .lightbox {
	    margin-bottom: 0;
	    img {
	        margin-bottom: 0;
	    }
	}
	div {
	    background: rgba(0,0,0,.06);
        padding: 10px 20px;
        a i {
            font-size: 80%;
            margin-left: 5px;
        }
	}
	&.hidden {
	    margin: 0;
	    display: inline;
	    width: 0 !important;
	    height: 0;
	    @include opacity(0);
	}
	margin-bottom: 30px;
    @media only screen and (max-width: 767px) {
        margin-bottom: 30px !important;
        @include transition-duration(0s);
    }
}


body.no-topmenu {
    #menu-top {
        height: 50px;
    }
    .language_switcher {
        left: auto;
        right: 10px;
    }
}

body.single-portfolio_item {
    .blog-content {
        margin-bottom: 40px;
    }
    .related-post {
        .lightbox, .fancybox, p {
            margin-bottom: 0;
        }
        > div {
    	    background: rgba(0,0,0,.06);
            padding: 10px 20px;
            a i {
                font-size: 80%;
                margin-left: 5px;
            }
        }
    }
}


body.page-template-template-sitemap-php {
    .sitemap {
        li {
            border-bottom: 1px solid rgba(0,0,0,.04);
            padding-bottom: 10px;
        }    
        i {
            margin: 2px 10px 0 0;
            float: left;
        }
        span {
            font-size: 80%;
        }
        div {
            overflow: hidden;
            span {
                display: block;
            }
        }
    }
}


.search-results {
    article.search-result-page {
        hr {
            margin-bottom: 10px;
        }
    }
}


// WP defaults
#body-content img {
    &.alignright,
    &.alignleft,
    &.aligncenter {
        width: auto;
    }
}

.shortcode-post {
    h4 {
        margin-bottom: 0;
    }
    small {
        font-style: italic;
        margin-bottom: 5px;
        display: block;
    }
    .lightbox, .fluid-width-video-wrapper {
        margin-bottom: 10px;
    }
    span {
        display: block;
        margin-bottom: 10px;
    }
    a.button i {
        float: none;
        margin-left: 5px;
        margin-right: 0;
    }
}



